<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ivan.gimnasio.presentation.controller.ListarSociosController">
    <stylesheets>
        <URL value="@/css/style.css" />
    </stylesheets>
    <TableView fx:id="tablaSocios" styleClass="table-view" prefHeight="400.0" prefWidth="700.0">
        <columns>
            <TableColumn fx:id="colNombre" prefWidth="100" text="Nombre" />
            <TableColumn fx:id="colApellido" prefWidth="100" text="Apellido" />
            <TableColumn fx:id="colDni" prefWidth="100" text="DNI" />
            <TableColumn fx:id="colActividad" text="Actividad" />
            <TableColumn fx:id="colEstadoCuota" prefWidth="100" text="Estado" />
            <TableColumn fx:id="colAcciones" prefWidth="100" text="Acciones" />
        </columns>
    </TableView>


</AnchorPane>
